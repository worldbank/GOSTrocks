ng","implicit":true,"key":"NEfxhALp72"},{"type":"paragraph","position":{"start":{"line":85,"column":1},"end":{"line":86,"column":1}},"children":[{"type":"strong","position":{"start":{"line":85,"column":1},"end":{"line":85,"column":1}},"children":[{"type":"text","value":"Community Impact","position":{"start":{"line":85,"column":1},"end":{"line":85,"column":1}},"key":"SGu6zQmFfP"}],"key":"ieSwh3YNM4"},{"type":"text","value":": A violation through a single incident or series\nof actions.","position":{"start":{"line":85,"column":1},"end":{"line":85,"column":1}},"key":"hlq9hse9Iu"}],"key":"TpGEe9ESad"},{"type":"paragraph","position":{"start":{"line":88,"column":1},"end":{"line":93,"column":1}},"children":[{"type":"strong","position":{"start":{"line":88,"column":1},"end":{"line":88,"column":1}},"children":[{"type":"text","value":"Consequence","position":{"start":{"line":88,"column":1},"end":{"line":88,"column":1}},"key":"ZKGjuXSlkV"}],"key":"SEvMOB9OPp"},{"type":"text","value":": A warning with consequences for continued behavior. No\ninteraction with the people involved, including unsolicited interaction with\nthose enforcing the Code of Conduct, for a specified period of time. This\nincludes avoiding interactions in community spaces as well as external channels\nlike social media. Violating these terms may lead to a temporary or\npermanent ban.","position":{"start":{"line":88,"column":1},"end":{"line":88,"column":1}},"key":"miUdi390Fe"}],"key":"pA5kRrvgGh"},{"type":"heading","depth":3,"position":{"start":{"line":95,"column":1},"end":{"line":95,"column":1}},"children":[{"type":"text","value":"3. Temporary Ban","position":{"start":{"line":95,"column":1},"end":{"line":95,"column":1}},"key":"W15VT8zUrT"}],"identifier":"id-3-temporary-ban","label":"3. Temporary Ban","html_id":"id-3-temporary-ban","implicit":true,"key":"HoSQDusnMs"},{"type":"paragraph","position":{"start":{"line":97,"column":1},"end":{"line":98,"column":1}},"children":[{"type":"strong","position":{"start":{"l